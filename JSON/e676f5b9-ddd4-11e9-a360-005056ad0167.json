e":["25-06-2019"],"EndYear":[2019],"EndMonth":[6],"EndDay":[25],"EndHour":[19],"EndMinute":[50],"ApplicationId":["6e164897-6008-4c48-b128-7bc0aed5d18a"],"NaceId":{},"NaceCode":{},"Latitude":{},"Longitude":{},"Translation":{"VersionId":["851ec68f-d3db-4557-8308-99b687d327e4"],"LanguageId":["EN"],"Title":["Fire in finished product warehouse"],"PlantName":{},"PlantAddress":{},"AdditionalComments":[""],"ConfidentialDescription":[""],"LessonsLearned":["&lt;p&gt;FIRE FIGHTING AND DETECTION SYSTEMS&lt;/p&gt;\n&lt;ul style=\"list-style-type: disc\"&gt;\n&lt;li&gt;Improvement of firefighting systems: sprinklers and others.&lt;/li&gt;\n&lt;li&gt;Evaluation of firefighting systems in other packaging areas in the plant.&lt;/li&gt;\n&lt;li&gt;Analysis the use of foam instead of water.&lt;/li&gt;\n&lt;/ul&gt;\n&lt;p&gt;STAFF TRAINING&lt;/p&gt;\n&lt;ul style=\"list-style-type: disc\"&gt;\n&lt;li&gt;Improvement of training of second intervention team.&lt;/li&gt;\n&lt;li&gt;Exercises with staff of the refinery nearby.&lt;/li&gt;\n&lt;li&gt;Training of outsourced staff.&lt;/li&gt;\n&lt;/ul&gt;\n&lt;p&gt;OTHER&lt;/p&gt;\n&lt;ul style=\"list-style-type: disc\"&gt;\n&lt;li&gt;Improvement of access in case of emergency.&lt;/li&gt;\n&lt;li&gt;Security cameras.&lt;/li&gt;\n&lt;li&gt;Report all incidents and minor accidents to 112 and make sure all the staffs knows direct telephone access to Emergency Coordination Center.&lt;/li&gt;\n&lt;li&gt;Prepare a room for official visits different from the emergency room.&lt;br&gt;&lt;br&gt;&lt;/li&gt;\n&lt;/ul&gt;"],"EmergencyResponce_Descr":["&lt;p&gt;The internal emergency response consisted of:&lt;/p&gt;\n&lt;ul style=\"list-style-type: disc\"&gt;\n&lt;li&gt;All human resources with specifics functions in the interior emergency plan.&lt;/li&gt;\n&lt;li&gt;First intervention vehicle.&lt;/li&gt;\n&lt;li&gt;Firefighting booths and equipment.&lt;/li&gt;\n&lt;li&gt;Foam containers.&lt;/li&gt;\n&lt;li&gt;Firefighting monitors.&lt;/li&gt;\n&lt;li&gt;Fire hydrant network.&lt;/li&gt;\n&lt;li&gt;Ambulance&lt;/li&gt;\n&lt;li&gt;Radiotelephones, megaphones, FACT 24 system.&lt;/li&gt;\n&lt;li&gt;Environmental monitoring network.&lt;/li&gt;\n&lt;/ul&gt;\n&lt;p&gt;The external support was:&lt;/p&gt;\n&lt;ul style=\"list-style-type: disc\"&gt;\n&lt;li&gt;Fire brigades of the Cadiz consortium. Ten teams in total withh 60 firefighters.&lt;/li&gt;\n&lt;li&gt;Firefighters from another nearby Seveso establishment.&amp;nbsp;&lt;br&gt;&lt;br&gt;&lt;/li&gt;\n&lt;/ul&gt;"],"IsActive":[true],"LastUserId":["6799492a-0079-4d05-aad1-8f4dce857337"],"LastUser":{"UserId":["6799492a-0079-4d05-aad1-8f4dce857337"],"Name":["Enrico"],"Surname":["GUAGNINI"],"IsVisibleSensibleData":[false]},"LastDate":["2020-02-26T14:49:51.32"],"ParentCompany":{},"Region":{},"Province":{},"City":{},"PostCode":{},"Concurrency":[0]},"LockedUserId":{},"LockedDate":{},"IsLocked":[false],"IsLockedByMe":[false],"LockedUser":{}},"AccidentProfile_Tab":{"ReportingAuthority":{},"ContactPerson":{},"Reasons":[{"IsChecked":false,"ItemId":"1","ItemDescription":"Substances involved: greater than 5% of quantity in Column 3 of Annex I"},{"IsChecked":false,"ItemId":"2","ItemDescription":"Injury to persons: &gt;= 1 fatalities, &gt;= 6 hospitalizing injuries etc."},{"IsChecked":false,"ItemId":"3","ItemDescription":"Immediate damage to the environment (according to Annex VI)"},{"IsChecked":true,"ItemId":"4","ItemDescription":"Damage to property: on-site &gt;2M &amp;euro;, off-site &gt; 0.5M &amp;euro;"},{"IsChecked":false,"ItemId":"5","ItemDescription":"Cross-border damage: transboundary accidents"},{"IsChecked":false,"ItemId":"6","ItemDescription":"Interesting for lessons learned."}],"AffectedCountries":{}},"AccidentDescription_Tab":{"AccidentDescription":{"DominoEffects":[false],"NatechEvents":[false],"TransboundaryEffects":[false],"Contractors":[false],"accTypeRelease":[0],"accTypeFire":[0],"accTypeExplosion":[0],"accTypeTransport":[0],"accTypeOther":{},"IsActive":[true],"LastUse